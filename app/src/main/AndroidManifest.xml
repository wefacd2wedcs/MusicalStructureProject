<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GenicAlbum"
            android:label="Playing Genic" />
        <activity
            android:name=".FeelAlbum"
            android:label="Playing Feel" />
        <activity
            android:name=".FinallyAlbum"
            android:label="Playing Finally" />
        <activity
            android:name=".BalladaAlbum"
            android:label="Playing Ballada" />
    </application>

</manifest>